{
  "name": "@gud/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "node --import tsx/esm node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts",
    "db:rollback": "node --import tsx/esm node_modules/knex/bin/cli.js migrate:rollback --knexfile knexfile.ts",
    "db:seed": "node --import tsx/esm node_modules/knex/bin/cli.js seed:run --knexfile knexfile.ts"
  },
  "dependencies": {
    "@gud/shared": "workspace:*",
    "@socket.io/redis-adapter": "^8.3.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "ioredis": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "mysql2": "^3.11.0",
    "nodemailer": "^8.0.1",
    "otpauth": "^9.5.0",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^7.0.9",
    "@types/qrcode": "^1.5.6",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
